unit uFuncoesDB;

interface

uses
  Data.DB, Data.SqlExpr

  function PegarCodigoAutoIncrement(sGenerator: string): Int64;

implementation

uses
  udmPrincipal;

function PegarCodigoAutoIncrement(sGenerator: string): Int64;
var
  qlPqPessoas: TSQLDataSet;
begin
  SELECT GEN_ID(NOME_DO_GENERATOR,0) FROM RDB$DATABASE
end;

end.
