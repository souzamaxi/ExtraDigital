      'N CADCIDADE CID ON (ENDE.IDCIDADE = CID.IDCIDADE)'#13#10'LEFT JOIN CAD' +
      'UF UF ON (ENDE.IDUF = UF.IDUF)'#13#10'WHERE ENDE.IDEMPRESA = :IDEMPRES' +
